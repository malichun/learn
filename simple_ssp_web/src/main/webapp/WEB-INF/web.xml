<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>upload_excel_zhifubao</servlet-name>
    <servlet-class>com.vlion.servlet.UploadExcelZhifubaoServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>predictxgb</servlet-name>
    <servlet-class>com.vlion.servlet.QueryPredictServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>upload_excel_taobao</servlet-name>
    <servlet-class>com.vlion.servlet.UploadExcelTaobaoServlet</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>upload_excel_zhifubao</servlet-name>
    <url-pattern>/upload_execel_zhifubao</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>upload_excel_taobao</servlet-name>
    <url-pattern>/upload_execel_taobao</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>predictxgb</servlet-name>
    <url-pattern>/predictxgb</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>process_excel.jsp</welcome-file>
  </welcome-file-list>

</web-app>
